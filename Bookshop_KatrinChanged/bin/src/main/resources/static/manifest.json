{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.sap.bookstoreTA.com.sap.bookstoreTA",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"

		},
		"dataSources": {
			"bookDataSource": {
				"uri": "/api/v1/book",
				"type": "JSON"

			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.sap.bookstoreTA.com.sap.bookstoreTA.view.app",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
			"config": {
			"sample": {
				"files": [
					"app.view.xml",
					"app.controller.js",
					"manifest.json"
				]
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.bookstoreTA.com.sap.bookstoreTA.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "bookDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}
		},

		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.bookstoreTA.com.sap.bookstoreTA.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Routeapp",
				"pattern": "Routeapp",
				"target": ["Targetapp"]
			}
		,{
				"name": "detail",
				"pattern": "detail/{isbn}",
				"target": ["detail"]
			}
			,{
				"name": "HomePage",
				"pattern": "",
				"target": ["Targetapp"]
			}
		],
			"targets": {
				"Targetapp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "app",
					"viewName": "app"
				},
				"detail": {
					"viewId":"detail",
					"viewName":"detail"
				}
				,
				"app": {
					"viewId":"app",
					"viewName":"app"
				}
			}
		}
	}
}